{
    "manifest_version": 3,
    "name": "Web Page Cloner",
    "version": "1.0",
    "description": "Clone the UI and UX of any web page",
    "permissions": [
      "activeTab",
      "downloads",
      "scripting",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }